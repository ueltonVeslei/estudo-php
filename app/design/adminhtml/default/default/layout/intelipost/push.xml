<?xml version="1.0" encoding="utf-8" ?>

<layout version="0.1.0">
    <adminhtml_push_orders_index>
        <reference name="content">
            <block type="push/adminhtml_orders" name="push_orders" />           
        </reference>
    </adminhtml_push_orders_index>

    <adminhtml_push_orders_printromaneio>
        <reference name="content">
            <block type="push/adminhtml_orders_romaneio" name="push_orders_romaneio" />
            <remove name="head" />
            <remove name="global_notices" />
            <remove name="header" />
            <remove name="menu" />
            <remove name="notifications" />
            <remove name="footer" />
            <remove name="profiler" />
        </reference>
    </adminhtml_push_orders_printromaneio>

    <adminhtml_push_orders_romaneiodetails>
        <reference name="head">
            <action method="addItem">
                <type>skin_js</type>
                <script>js/intelipost/jquery-1.11.2.min.js</script>
            </action>
            <action method="addItem">
                <type>skin_js</type>
                <script>js/intelipost/noconflict.js</script>
            </action>
        </reference>
        <reference name="content">
            <block type="push/adminhtml_orders_romaneio_details" name="push_orders_romaneio_details" />       
        </reference>
    </adminhtml_push_orders_romaneiodetails>
    <adminhtml_push_nfes_index>
        <reference name="content">
            <block type="push/adminhtml_nfes" name="push_nfes" />
        </reference>
    </adminhtml_push_nfes_index>
    <adminhtml_push_trackings_index>
        <reference name="content">
            <block type="push/adminhtml_trackings" name="push_trackings" />
        </reference>
    </adminhtml_push_trackings_index>
</layout>

