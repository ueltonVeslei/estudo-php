<layout version="0.1.0">
	
	<checkout_cart_index>
		<reference name="head">
			<block type="core/text" name="external_js" ifconfig="quote/debug"> 
				<action method="setText">
					<text>
						<![CDATA[<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/highlight.js/8.4/highlight.min.js"></script>]]>
						<![CDATA[<link href="//cdnjs.cloudflare.com/ajax/libs/highlight.js/8.4/styles/default.min.css" type="text/css" rel="stylesheet" /></script>]]>
						<![CDATA[<script type="text/javascript">hljs.initHighlightingOnLoad();</script>]]>
					</text> 
				</action> 
			</block>	
		</reference>
		
		<reference name="root">
			<block type="quote/profiler" output="toHtml" name="intelipost_profiler"/>
		</reference>
	</checkout_cart_index>

