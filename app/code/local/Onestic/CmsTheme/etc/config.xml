<?xml version="1.0"?>
<config>
    <modules>
        <Onestic_CmsTheme>
            <version>0.1.0</version>
        </Onestic_CmsTheme>
    </modules>
    <frontend>
        <routers>
            <cms>
                <args>
                    <modules>
                        <Onestic_CmsTheme before="Mage_Cms">Onestic_CmsTheme</Onestic_CmsTheme>
                    </modules>
                </args>
            </cms>
        </routers>
    </frontend>
    <global>
      <blocks>
          <adminhtml>
              <rewrite>
                  <catalog_search_grid>Onestic_CmsTheme_Block_Catalog_Search_Grid</catalog_search_grid>
              </rewrite>
          </adminhtml>
      </blocks>
  </global>
</config>
