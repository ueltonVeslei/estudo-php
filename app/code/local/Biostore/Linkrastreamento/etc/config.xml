<config>
    <modules>
        <Biostore_Linkrastreamento>
            <version>0.1.0</version>
        </Biostore_Linkrastreamento>
    </modules>
    <global>
        <models>
            <linkrastreamento>
                <class>Biostore_Linkrastreamento_Model</class>
            </linkrastreamento>
        </models>
        <blocks>
            <linkrastreamento>
                <class>Biostore_Linkrastreamento_Block</class>
            </linkrastreamento>
        </blocks>
        <resources>
            <linkrastreamento_setup>
                <setup>
                    <module>Biostore_Linkrastreamento</module>
                    <class>Biostore_Linkrastreamento_Model_Resource_Eav_Mysql4_Setup</class>
                </setup>
            </linkrastreamento_setup>
        </resources>
    </global>
    <frontend> 
        <routers>
            <linkrastreamento>
                <use>standard</use>
                <args>
                    <module>Biostore_Linkrastreamento</module>
                    <frontName>linkrastreamento</frontName>
                </args>
            </linkrastreamento>
        </routers>
        <layout>
            <updates>
                <linkrastreamento>
                    <file>linkrastreamento.xml</file>
                </linkrastreamento>
            </updates>
        </layout>
    </frontend>
</config>
