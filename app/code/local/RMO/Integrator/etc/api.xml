<config>
    <api>
        <resources>
            <integrator_product translate="title" module="rmointegrator">
                <title>RMO INTEGRATOR</title>
                <model>rmointegrator/catalog_product_api</model>
                <acl>integrator/product</acl>
                <methods>
                    <items translate="title" module="rmointegrator">
                      <title>Retrieve list of products</title>
                      <acl>integrator/product/list</acl>
                    </items>
                    <itemsPaged translate="title" module="rmointegrator">
                      <title>Retrieve list of products using pagination</title>
                      <acl>integrator/product/list</acl>
                    </itemsPaged>
                    <urls translate="title" module="rmointegrator">
                      <title>Retrieve url of products</title>
                      <acl>integrator/product/list</acl>  
                    </urls>                    
                    <listCreated translate="title" module="rmointegrator">
                        <title>Retrieve list of created products</title>
                        <acl>integrator/product/listCreated</acl>
                    </listCreated>
                    <listUpdated translate="title" module="rmointegrator">
                        <title>Retrieve list of updated products</title>
                        <acl>integrator/product/listUpdated</acl>
                    </listUpdated>
                    <listDeleted translate="title" module="rmointegrator">
                        <title>Retrieve list of delted products</title>
                        <acl>integrator/product/listDeleted</acl>
                    </listDeleted>
                    <listCreatedCount translate="title" module="rmointegrator">
                        <title>Count number of created products</title>
                        <acl>integrator/product/listCreatedCount</acl>
                    </listCreatedCount>
                    <listUpdatedCount translate="title" module="rmointegrator">
                        <title>Count number of updated products</title>
                        <acl>integrator/product/listUpdatedCount</acl>
                    </listUpdatedCount>
                    <listDeletedCount translate="title" module="rmointegrator">
                        <title>Count number of updated products</title>
                        <acl>integrator/product/listDeletedCount</acl>
                    </listDeletedCount>
                    <confirmListCreatedReceived translate="title" module="rmointegrator">
                        <title>Confirms that the integrator has received the created products</title>
                        <acl>integrator/product/confirmListCreatedReceived</acl>
                    </confirmListCreatedReceived>
                    <confirmListUpdatedReceived translate="title" module="rmointegrator">
                        <title>Confirms that the integrator has received the updated products</title>
                        <acl>integrator/product/confirmListUpdatedReceived</acl>
                    </confirmListUpdatedReceived>
                    <confirmListDeletedReceived translate="title" module="rmointegrator">
                        <title>Confirms that the integrator has received the updated products</title>
                        <acl>integrator/product/confirmListDeletedReceived</acl>
                    </confirmListDeletedReceived>
                    <importCsv>
                        <title>Import products trough CSV file</title>
                        <acl>integrator/product/importProductCsv</acl>
                    </importCsv>
                </methods>
            </integrator_product>
            <integrator_order translate="title" module="rmointegrator">
                <title>RMO INTEGRATOR ORDER</title>
                <model>rmointegrator/sale_order_api</model>
                <acl>integrator/order</acl>
                <methods>
                    <info translate="title" module="rmointegrator">
                        <title>Extend the defaul saleInfo with extra fields</title>
                        <acl>integrator/order/info</acl>
                    </info>
                    <listStatus translate="title" module="rmointegrator">
                        <title>List available order status</title>
                        <acl>integrator/order/listStatus</acl>
                    </listStatus>
                    <paymentMethods translate="title" module="rmointegrator">
                       <title>List all payment methods</title>
                       <acl>integrator/order/paymentMethods</acl>
                    </paymentMethods>
                    <shippingMethods translate="title" module="rmointegrator">
                       <title>List all payment methods</title>
                       <acl>integrator/order/shippingMethods</acl>
                    </shippingMethods>
                    <create translate="title" module="rmointegrator">
                       <title>Create a new Order</title>
                       <acl>integrator/order/create</acl>
                    </create>
                    <update translate="title" module="rmointegrator">
                       <title>Update an Order</title>
                       <acl>integrator/order/update</acl>
                    </update>
                    <invoice translate="title" module="rmointegrator">
                       <title>Invoice a order</title>
                       <acl>integrator/order/create</acl>
                    </invoice>
                    <estimateShipping translate="title" module="rmointegrator">
                       <title>Estimate shipping price and delivery time</title>
                       <acl>integrator/order/estimateShipping</acl>
                    </estimateShipping>
                    <estimateShippingPerItem translate="title" module="rmointegrator">
                       <title>Estimate shipping price and delivery time per item</title>
                       <acl>integrator/order/estimateShipping</acl>
                    </estimateShippingPerItem>
                    <listShipments  translate="title" module="rmointegrator">
                        <title>List order's shipment</title>
                       <acl>integrator/order/listShipments</acl>
                    </listShipments>
                </methods>
            </integrator_order>
        </resources>
        <v2>
            <resources_function_prefix>
                <integrator_product>integratorProduct</integrator_product>
                <integrator_order>integratorOrder</integrator_order>
            </resources_function_prefix>
        </v2>
        <acl>
            <resources>
                <integrator translate="title" module="rmointegrator">
                    <title>RMO Integrator</title>
                    <sort_order>2000</sort_order> 
                    <order translate="title" module="rmointegrator">
                        <title>Order</title>
                        <info translate="title" module="rmointegrator">
                            <title>Order Info</title>
                        </info>
                        <listStatus translate="title" module="rmointegrator">
                            <title>List Status</title>
                        </listStatus >
                        <paymentMethods translate="title" module="rmointegrator">
                            <title>List Payment Methods</title>
                        </paymentMethods>
                        <shippigMethods translate="title" module="rmointegrator">
                            <title>List Shipping Methods</title>
                        </shippigMethods>
                        <create translate="title" module="rmointegrator">
                            <title>Create a new Order</title>
                        </create>
                        <update translate="title" module="rmointegrator">
                            <title>Update an Order</title>
                        </update>
                        <invoice translate="title" module="rmointegrator">
                            <title>Invoice a order</title>
                        </invoice>
                        <estimateShipping translate="title" module="rmointegrator">
                            <title>Estimate Shipping price and delivery time</title>
                        </estimateShipping>
                        <estimateShippingPerItem translate="title" module="rmointegrator">
                            <title>Estimate Shipping price and delivery time per item</title>
                        </estimateShippingPerItem>
                        <listShipments translate="title" module="rmointegrator">
                            <title>List order's shipment</title>
                        </listShipments>
                    </order>
                    <product translate="title" module="rmointegrator">
                        <title>Product</title>
                        <items translate="title" module="rmointegrator">
                            <title>List Available products</title>
                        </items> 
                        <itemsPaged translate="title" module="rmointegrator">
                            <title>List Available products using pagination</title>
                        </itemsPaged>
                        <urls translate="title" module="rmointegrator">
                            <title>List product's urls</title>
                        </urls> 
                        <listCreated translate="title" module="rmointegrator">
                            <title>List created products</title>
                        </listCreated>  
                        <listUpdated translate="title" module="rmointegrator">
                            <title>List updated products</title>
                        </listUpdated>
                        <listDeleted translate="title" module="rmointegrator">
                            <title>List deleted products</title>
                        </listDeleted>
                        <listCreatedCount translate="title" module="rmointegrator">
                            <title>Count number of created products</title>
                        </listCreatedCount>
                        <listUpdatedCount translate="title" module="rmointegrator">
                            <title>Count number of updated products</title>
                        </listUpdatedCount>
                        <listDeletedCount translate="title" module="rmointegrator">
                            <title>Count number of deleted products</title>
                        </listDeletedCount>
                        <confirmListCreatedReceived translate="title" module="rmointegrator">
                            <title>Confirms that the integrator has received the created products</title>
                        </confirmListCreatedReceived>
                        <confirmListUpdatedReceived translate="title" module="rmointegrator">
                            <title>Confirms that the integrator has received the updated products</title>
                        </confirmListUpdatedReceived>
                        <confirmListDeletedReceived translate="title" module="rmointegrator">
                            <title>Confirms that the integrator has received the deleted products</title>
                        </confirmListDeletedReceived>
                        <importCsv>
                            <title>Import products from csv file</title>
                        </importCsv>
                    </product>                   
                </integrator>
            </resources>
        </acl>
    </api>
</config>